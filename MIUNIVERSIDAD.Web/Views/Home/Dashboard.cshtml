@model MIUNIVERSIDAD.Web.ViewModel.DashboardViewModel
@{
    ViewBag.Title = "Dashboard";
}

<div class="row">
    <br />
    <br />
    <br />
    <div class="col-lg-4 col-md-4">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-3">
                        <i class="fa fa-book fa-5x"></i>
                    </div>
                    <div class="col-xs-9 text-right">
                        <div class="huge">@Model.NroUniversidades</div>
                        <div>Universidades</div>
                    </div>
                </div>
            </div>
            <a href="#">
                <div class="panel-footer">
                    <a href="@Url.Action("LstUniversidad", "Region")">
                        <span class="pull-left">Ver más</span>
                        <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span></a>
                        <div class="clearfix"></div>
</div>
            </a>
        </div>
    </div>
   

</div>

